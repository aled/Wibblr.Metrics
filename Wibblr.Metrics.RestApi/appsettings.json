{
  "Plugins": [
    "CockroachDb",
    "SqlServer"
  ],
  "DatabaseType": "SqlServer",
  "Database": {
    "Connection": {
      "ConnectionString": "Data Source=(localdb)\\mssqllocaldb;Initial Catalog=MetricsExamples;Integrated Security=SSPI"
    },
    "Tables": {
      "Counter": "MetricsCounter",
      "Histogram": "MetricsHistogram",
      "Event": "MetricsEvent",
      "Profile": "MetricsProfile"
    }
  },
  "XDatabaseType": "CockroachDb",
  "XDatabase": {
    "Connection": {
      "Host": "xxx.cockroachlabs.cloud",
      "Port": 12345,
      "Username": "xxx",
      "Password": "xxx",
      "Database": "xxx.defaultdb",
      "RequireSsl": true,
      "CaCertFile": "CockroachDb-certificate-authority.crt"
    },
    "Tables": {
      "Counter": "MetricsCounter",
      "Histogram": "MetricsHistogram",
      "Event": "MetricsEvent",
      "Profile": "MetricsProfile"
    }
  },
  "MetricsWriter": {
    "BatchSize": 1000,
    "MaxQueuedRows": 10000
  },
  "MetricsCollector": {
    "WindowSize": "00:00:02",
    "FlushInterval": "00:00:05",
    "IgnoreEmptyBuckets": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
